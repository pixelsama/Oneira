{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "store:default",
    "fs:default",
    "dialog:default",
    "shell:default",
    {
      "identifier": "fs:scope",
      "allow": ["$DOCUMENT/Oneiria/**"]
    }
  ]
}
